

<!--  navbar -->
<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
    <%= link_to "Store Home", store_path, class: "navbar-brand" %>
    </div>
    <div class="navbar-collapse collapse">
      <ul class="nav navbar-nav">
        <!--  nav here -->
        <% if !!@cart %>
          <li> <%= link_to "Cart", cart_path(current_cart) %></li>
        <% end %>
        <% if user_signed_in? %>
          <li class="navbar-text">Signed in as <%= current_user.email %></li>
          <li><%= link_to 'Sign out', destroy_user_session_path, method: 'delete' %></li>
        <% else %>
          <li><%= link_to 'Sign in', new_user_session_path, method: 'get' %></li>
          <li><%= link_to 'Sign up', new_user_registration_path, method: 'get' %></li>
        <% end %>

      </ul>
    </div><!--/.nav-collapse -->
  </div>
</div>
